		function checkPlatForm(){
			var platId = $("#platId").val();
			if($.trim(platId) == ""){
				$("#platformMsg").html("请选择公众平台");
				return false;
			}
			$("#platformMsg").html("");
			return true;
		}
	
		function submitStudentForm(){
	
			if($("#stuNo_").val().trim() == ""){
				alert("请填写学生学号！");
				return;
			} 
			//length 判断
			if($("#stuName_").val().length == "" ){
				alert('请填写学生姓名！');
				return;
			}
			
			document.forms[0].submit();
		}
