	<script type="text/javascript">
	<![CDATA[
		function exportExcl() {
			var commentLevels = $("#commentLevelsId").val();
			var commentContent = $("#commentContentId").val();
			var channels = $("#channelsId").val();
			var shops = $("#shopsId").val();
			var brands = $("#brandsId").val();
			var dispatchOperators = $("#dispatchOperatorsId").val();
			var replyStates = $("#replyStatesId").val();
			var commentStartTime = $("#commentStartTimeId").val();
			var commentEndTime = $("#commentEndTimeId").val();
			var createStartTime = $("#createStartTimeId").val();
			var createEndTime = $("#createEndTimeId").val();
			var replyStartTime = $("#replyStartTimeId").val();
			var replyEndTime = $("#replyEndTimeId").val();
			var userName = $("#userNameId").val();
			var goodsName = $("#goodsNameId").val();
			var tagName = $("#tagNameId").val();

			var src = "/ump/voccomments/exportExcle" + "?commentLevels="
					+ commentLevels + "&commentContent=" + commentContent
					+ "&channels=" + channels 
					+ "&shops=" + shops
					+ "&brands=" + brands 
					+ "&dispatchOperators="+dispatchOperators 
					+ "&replyStates=" + replyStates
					+ "&commentStartTime=" + commentStartTime
					+ "&commentEndTime=" + commentEndTime
					+ "&createStartTime=" + createStartTime
					+ "&createEndTime=" + createEndTime
					+ "&replyStartTime=" + replyStartTime
					+ "&replyEndTime=" + replyEndTime
					+ "&userName=" + userName 
					+ "&goodsName=" + goodsName
					+ "&isWebSearch=" + true;

			window.location.href = src;
		}

		/**
		 * 关闭所有
		 */
		function closeAll(searchDivId, fadeId) {
			$("#" + searchDivId).toggle();
			$("#" + fadeId).toggle();
		}

		/**
		 * 查询条件显示
		 */
		function searchPage(searchDivId, fadeId) {
			$("#" + searchDivId).toggle();
			$("#" + fadeId).toggle();
		}


		/**
		 *显示回复页面
		 */
		function showReply(commentIds) {
			var param = {
				commentIds : commentIds
			};
			$.ajax({
				url : "/ump/voccomments/getReolyPage",
				type : "POST",
				data : param,
				error : function(msg) {
					alert("请求错误! ");
				},
				success : function(text) {
					searchPage("showReplyId", "fade");
					showtk("showReplyId");
				}
			});
		}

		/**
		 *批量回复
		 */
		function showBatchReply() {
			var commentIds = new Array();
			var brandId = '';
			var commentLevel;
			var flag = true;
			$("[name = checkbox]:checkbox").each(function() {
				if ($(this).prop('checked')) {
					var jsonStr = eval("(" + $(this).val() + ")");
					if (brandId == '') {
						brandId = jsonStr.brandId;
					} else {
						if (brandId != jsonStr.brandId) {
							alert("请选择同一品牌!");
							flag = false;
							return false
						}
					}
					
					if(commentLevel == null || commentLevel == undefined){
						commentLevel = jsonStr.commnetLevel;
					}else{
						if(commentLevel != jsonStr.commnetLevel){
							alert("请选择同一评论类型!");
							flag = false;
							return false
						}
					}

					if (jsonStr.replyState == '待回复') {
						commentIds.push(jsonStr.commentId);
					} else {
						alert("请选择回复状态为待回复!");
						flag = false;
						return false
					}
				}
			});

			if (!flag) {
				return;
			}

			if (commentIds.length == 0) {
				alert("请选择评论!");
				return;
			}

			displayReplyPage(commentIds,1);
			
		}
		
		 //显示待回复数据
		 function displayReplyPage(commentIds,type){ //type 0 单条回复，1批量回复
			 if(!isLabelLimit()){
				alert("最多只能保留8个待处理页面!");
			    return;
			  }
			 
			   var labelId; //回复页面标签ID
			   if(type == 0){
				   labelId = commentIds;
			     //先判断打开的标签里是否包含当前的页面
			     //如果包含，直接显示页签，否则新打开一个页签
			       var commentIdArr = [commentIds];
				   labelId = getShownTabId(commentIdArr);
				   if(labelId){ //在已经打开的页签里，直接显示，不执行下面的代码
					  loadTable(labelId); 
					  return;
				   }else{ //如果没有不包含在打开的标签里
					  labelId = commentIds;
					  saveObject(labelId);
				   }
				 
			   }else{
				   //1  如果批量回复里有其它打开过的页签
				   //2 批量回复没有其它打开过的页签
				   //3 批量回复没有其它打开的页签
				   labelId = getShownTabId(commentIds);
				   if(labelId){
					  if(commentIds.length == 1){//只有一条批量回复时，直接显示打开的页签
						 loadTable(labelId);
					     return;
					  }else{
						alert("批量回复里包含已经打开的页签!");
					    return;
					  }
					  
				   }else{ //如果批量回复里不包含在打开的标签里,显示新标签
					  labelId = commentIds[0];
					  saveBatchObject(commentIds[0],commentIds);
				   }
				   commentIds = commentIds.join();
			   }
		    
		       if(!labelId){
		    	 return;	   
		       }
		 
			   showCurrentTab("commentId_"+labelId);
			   
			   var param = {
			    commentIds : commentIds
			   };
			   
				$.ajax({
					url : "/ump/voccomments/getRelyPage",
					type : "POST",
					data : param,
					error : function(msg) {
						alert("请求错误! ");
					},
					success : function(text) {
						$("#showReplyResult").html(text);
						showVocTabBar(labelId);
						changeTab();
					}
				});
		  }

		/**
		 * 发送回复内容
		 */
		function sendReply() {
			var commentIds = new Array();
			$("input[name='commnetId']").each(function() {
			  if($('#reply_'+$(this).val()).prop("checked")) {
			    commentIds.push($(this).val());
			  }
			});
            
			if(commentIds.length == 0){
				alert("请选中要回复的评论!");
				return;
			}
			
			var replyContent = $("#replyContentId").val();
			if (replyContent == "") {
				alert("回复内容不得为空！");
				return;
			}

			var param = {
				commentIds : commentIds.join(),
				replyContent : replyContent
			};

			$.ajax({
				url : "/ump/voccomments/sendReply",
				type : "POST",
				data : param,
				error : function(msg) {
					alert("请求错误! ");
				},
				success : function(text) {
					var jsonStr = eval("(" + text + ")");
					if (jsonStr.success) {
						alert("回复成功!");
						queryComment($("#page").val());
					} else {
						alert("回复失败!");
					}
					
					if(commentIds.length == 1){
					   var tabId = 'commentId_'+commentIds[0];
					   closeTable(tabId);	
					}else{ 
					  var tabId = 'commentId_'+commentIds[0];
					  closeTable(tabId);
						
					}
					
				}
			});

		}

		/**
		 *修改回复状态
		 */
		function changeReplyState(commentIds) {
		   var param = {
					commentIds : commentIds
				};
		   
				$.ajax({
					url : "/ump/voccomments/changeReplyState",
					type : "POST",
					data : param,
					error : function(msg) {
						alert("请求错误! ");
					},
					success : function(text) {
						var jsonStr = eval("(" + text + ")");
						if (jsonStr.success) {
							alert("修改成功!");
							queryComment($("#page").val());
						} else {
							alert("修改失败!");
						}

					}
				});
		}
		
		
		/**
		 * 回复状态修改检查
		 */
		function checkChangeReplyState(commentId){
			var str = $("#"+commentId).val();
			var jsonStr = eval("("+str+")");
			 var replySateStr = jsonStr.replyState;
			 replySateStr = replySateStr=='待回复'?'不回复':'待回复';
			 
			 if(confirm('确定把状态改为'+replySateStr+'吗')){
			   changeReplyState(commentId);	 
			 }
		}

		/**
		 * 批量不回复
		 */
		function batchChangeReplyState() {
			var commentIds = new Array();
			var flag = true;
			var defalutState = "";
			var replySateStr = "";
			$("[name = checkbox]:checkbox").each(function() {
				if ($(this).prop('checked')) {
					var jsonStr = eval("(" + $(this).val() + ")");
					replySateStr = jsonStr.replyState;
                     if(defalutState == ""){
                       defalutState = jsonStr.replyState;
                     }else{
                      if(defalutState != jsonStr.replyState){
                    	alert("请选择同一回复状态!!!");
  						flag = false;
  						return false
                      }
                    	 
                     }
                     
					
					if (jsonStr.replyState != '已回复') {
						commentIds.push(jsonStr.commentId);
					} else {
						alert("请选择回复状态的评论");
						flag = false;
						return false
					}
				}
			});

			if (!flag) {
				return;
			}

			if (commentIds.length == 0) {
				alert("请选择评论!");
				return;
			}
			
			 replySateStr = replySateStr=='待回复'?'不回复':'待回复';
			 if(confirm('确定把状态改为'+replySateStr+'吗')){
			   changeReplyState(commentIds.join());
			 }
			
		}
		
		

		/**
		 *调用模板
		 */
		function callTemplate(brandId,commentLevel) {
			 $("#showTemplateId").toggle();
			 createTree(brandId,commentLevel);
			 showtk("showTemplateId");
		}
		
		function addOnclickEvent(){
			$("#treeDemo7").find("a[class='level3'] span[class != 'button ico_docu']").each(function(){
				$(this).attr("onclick","chooseTemplatContent('"+$(this).text()+"')");
			});
		
