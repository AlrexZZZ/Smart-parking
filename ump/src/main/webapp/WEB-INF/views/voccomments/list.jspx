  */
 function saveWorkOrder(){
   var commentId = $("#w_showWorkOrderCommentId").val();
   var updateReason = $("#updateReasonId").val();
   var emailGroupId = $("#w_emallGroupId").val();
   var workOrderTypeId = $("#w_workOrderTypeId").val();
   var workOrderEmergency = $("#w_emergencyLevelId").val();
   
    if(null == updateReason ||'' == updateReason){
    	alert("请输入升级原因!");
    	return;
    }
    
    if(emailGroupId == ''){
    	alert("请选择邮件组!");
    	return;
    }
    
    if(workOrderTypeId == ''){
    	alert("请选择工单类型!");
    	return;
    }
    
    if(workOrderEmergency == ''){
    	alert("请选择优先级!");
    	return;
    }
    
    
   
	 var param = {
			 commentId : commentId,
			 updateReason:updateReason,
			 emailGroupId:emailGroupId,
			 workOrderEmergency:workOrderEmergency,
			 workOrderTypeId:workOrderTypeId	 
			};
	 
			$.ajax({
				url : "/ump/voccomments/saveWorkOrder",
				type : "POST",
				data : param,
				error : function(msg) {
					alert("请求错误! ");
				},
				success : function(text) {
					var jsonStr = eval("(" + text + ")");
					if (jsonStr.success) {
						alert("升级成功!");
						closeAll('workOrderUpdateId','fade');
					} else {
						alert("修改失败!");
					}

				}
			});
	 
 }
 
